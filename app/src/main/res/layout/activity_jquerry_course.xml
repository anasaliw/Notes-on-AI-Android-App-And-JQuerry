<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2B2929"
    tools:context=".jquerry_course">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="407dp"
        android:layout_height="57dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="4dp"
        android:layout_marginRight="4dp"
        android:background="#ACB7F4"
        android:fontFamily="cursive"
        android:text="JQuerry Course"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#1B1A1A"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/j1"
        android:layout_width="303dp"
        android:layout_height="32dp"
        android:layout_marginTop="40dp"
        android:background="#C3CBF6"
        android:text="Chapter 1: JQuerry Home"
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/j2"
        android:layout_width="303dp"
        android:layout_height="32dp"
        android:layout_marginTop="28dp"
        android:background="#C3CBF6"
        android:text="Chapter 2: JQuerry Intro"
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/j1" />

    <TextView
        android:id="@+id/j3"
        android:layout_width="303dp"
        android:layout_height="32dp"
        android:layout_marginTop="28dp"
        android:background="#C3CBF6"
        android:text="Chapter 3: Get Started"
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/j2" />

    <TextView
        android:id="@+id/j4"
        android:layout_width="303dp"
        android:layout_height="32dp"
        android:layout_marginTop="28dp"
        android:background="#C3CBF6"
        android:text="Chapter 4: JQuerry Syntax"
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/j3" />

    <TextView
        android:id="@+id/j5"
        android:layout_width="303dp"
        android:layout_height="32dp"
        android:layout_marginTop="28dp"
        android:background="#C3CBF6"
        android:text="Chapter 5: JQuerry Selectors "
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/j4" />

    <TextView
        android:id="@+id/j6"
        android:layout_width="303dp"
        android:layout_height="32dp"
        android:layout_marginTop="28dp"
        android:background="#C3CBF6"
        android:text="Chapter 6: Events"
        android:textSize="18sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/j5" />

</androidx.constraintlayout.widget.ConstraintLayout>